S := 256
N := 512
PI := 3.141592

screensize(S)
worldcoords(-S/2, S/2,-S/2, S/2)
pencolor("#ffffff")
clear()

pencolor("#000000")

i := 0
while (i < N) {
	t := float(i) / float(N)
	d := t*S*0.4 + 10
	a := t * PI * 2 * 20
	
	x := cos(a)*d
	y := sin(a)*d
	r := t * 5

	circle(x, y, r)

	i = i + 1
}

fill()

snapshot("./examples/spiral.png")